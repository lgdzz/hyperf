{
  "name": "lgdz/hyperf",
  "version": "2.2.3",
  "description": "tools class",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "hyperf/cache": "~2.2.0",
    "hyperf/command": "~2.2.0",
    "hyperf/config": "~2.2.0",
    "hyperf/db-connection": "~2.2.0",
    "hyperf/framework": "~2.2.0",
    "hyperf/guzzle": "~2.2.0",
    "hyperf/http-server": "~2.2.0",
    "hyperf/logger": "~2.2.0",
    "hyperf/memory": "~2.2.0",
    "hyperf/process": "~2.2.0",
    "hyperf/database": "~2.2.0",
    "hyperf/redis": "~2.2.0",
    "hyperf/async-queue": "~2.2.0",
    "hyperf/model-cache": "~2.2.0",
    "hyperf/paginator": "^2.2",
    "hyperf/validation": "^2.2",
    "hyperf/filesystem": "^2.2",
    "ext-json": "*",
    "overtrue/wechat": "~5.0",
    "lgdz/factory": "~2.0.2"
  },
  "autoload": {
    "psr-4": {
      "lgdz\\hyperf\\": "src"
    },
    "files": [
      "../../../app/functions.php"
